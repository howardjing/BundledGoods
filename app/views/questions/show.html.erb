<h1><%= @question.name %></h1>
<p><%= render @question %></p>

<div class="row" style="padding-top: 40px;">
  <div class="span1">
    <%= @question.is_first? ? link_to('<<Back', @question.previous_path) : "<<Back" %>
  </div>
  <%= link_to 'Next>>', @question.next_path, :class => "offset1" %>
</div>
