<!DOCTYPE html>
<html>
<head>
  <title>BundledGoods</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="container">
  <% flash.each do |key, value| %>
    <div class="alert <%= bootstrapped(key) %>"><%= value %></div>
  <% end %>

  <%= mathjax_tag %>

  <%= "Welcome #{current_user.name}" if !current_user.nil? %>
  <%= yield %>
</div>

</body>
</html>
